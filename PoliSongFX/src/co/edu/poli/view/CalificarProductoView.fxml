<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.poli.controller.CalificarProductoController">

    <TableView fx:id="tablaProductos" layoutX="20" layoutY="20" prefHeight="300" prefWidth="560">
        <columns>
            <TableColumn fx:id="colIdDetalle" prefWidth="80" text="ID Detalle" />
            <TableColumn fx:id="colNombreProducto" prefWidth="200" text="Producto" />
            <TableColumn fx:id="colCantidad" prefWidth="80" text="Cantidad" />
            <TableColumn fx:id="colPrecio" prefWidth="100" text="Precio" />
        </columns>
    </TableView>

    <HBox layoutX="20" layoutY="340" spacing="10">
        <Button fx:id="btnCalificar" onAction="#calificarProducto" text="Calificar Producto" />
        <Button fx:id="btnVolver" onAction="#volverMenu" prefHeight="25.0" prefWidth="69.0" text="Volver" />
    </HBox>

</AnchorPane>
